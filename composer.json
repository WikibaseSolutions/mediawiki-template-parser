{
  "name": "wikibase-solutions/mediawiki-template-parser",
  "description": "A parser to parse MediaWiki templates.",
  "type": "library",
  "keywords": [
    "parser",
    "mediawiki"
  ],
  "homepage": "https://github.com/WikibaseSolutions/mediawiki-template-parser",
  "readme": "https://github.com/WikibaseSolutions/mediawiki-template-parser/README.md",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Marijn van Wezel",
      "email": "marijnvanwezel@gmail.com",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/WikibaseSolutions/mediawiki-template-parser/issues",
    "source": "https://github.com/WikibaseSolutions/mediawiki-template-parser"
  },
  "require": {
    "php": ">=7.4"
  },
  "require-dev": {
    "infection/infection": "^0.21.5",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "WikibaseSolutions\\MediaWikiTemplateParser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WikibaseSolutions\\MediaWikiTemplateParser\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "platform": {
      "php": "7.4.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
